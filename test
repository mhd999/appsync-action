cat input.json | ENV=development go run cmd/out/main.go
cat input.json | docker run -i --rm --entrypoint /opt/resource/out teliaoss/appsync-resource:dev opt/resource
cat input.json | docker run -i --rm --entrypoint /opt/resource/in teliaoss/appsync-resource:dev
cat input.json | docker run -i --rm --entrypoint /opt/resource/check teliaoss/appsync-resource:dev